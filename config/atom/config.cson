"*":
  "atom-package-deps":
    ignored: [
      "prettier-atom"
      "linter-eslint"
    ]
  core:
    autoHideMenuBar: true
    customFileTypes:
      "source.ini": [
        ".hgrc"
        ".buckconfig"
        ".flowconfig"
      ]
      "source.json": [
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "tree-view"
    ]
    telemetryConsent: "no"
  editor:
    preferredLineLength: 100
  "exception-reporting":
    userId: "9c926188-0dbd-4382-910c-b2bafe8d5d47"
  nuclide:
    "atom-ide-code-format": {}
    installRecommendedPackages: true
    "nuclide-flow":
      canUseFlowBin: true
    "nuclide-home":
      showHome: false
    use: {}
  "prettier-atom":
    formatOnSaveOptions:
      enabled: true
    prettierOptions:
      parser: "flow"
    useEditorConfig: false
    useEslint: true
  "todo-show":
    sortBy: "Path"
  welcome:
    showOnStartup: false
".reason.source":
  editor:
    preferredLineLength: 110
